@model List<App.Form.ViewModels.LinkToSubmission>;

@{
    ViewData["Title"] = "Case Study Submission List";
}
<div class="container">
    <h1>@ViewData["Title"]</h1>



</div>



    <div class="container">
        <div>
        @if (Model.Any())
        {
            foreach (var link in Model)
            {
                <ul>
                    <li>
                        <a asp-controller="Dossier" asp-action="Index"
                           asp-route-id="@link.Id">@link.Organisation</a>
                    </li>
                </ul>
            }
        }
        </div>
    </div>

